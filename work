try {
    // Check if groups is valid and iterable
    if (groups != null && groups instanceof java.util.List) {

        for (Object obj : (List) groups) {
            if (obj instanceof Map) {
                Map group = (Map) obj;

                String perm = "";
                if (group.get("permissions") != null) {
                    perm = group.get("permissions").toString().toLowerCase();
                }

                // Filter keywords
                if (perm.contains("administration") ||
                    perm.contains("approval queue") ||
                    perm.contains("client onboarding") ||
                    perm.contains("books closed") ||
                    perm.contains("cusip inquiry") ||
                    perm.contains("masterfile") ||
                    perm.contains("pricing")) {

                    log.error("‚úÖ Keeping group: " + perm);
                    filtered.add(group);
                } else {
                    log.error("‚ùå Removing group: " + perm);
                }
            }
        }
    } else {
        log.error("‚ö†Ô∏è 'groups' is null or not a List. Type: " + 
                  (groups == null ? "null" : groups.getClass().getName()));
    }
} catch (Exception e) {
    log.error("üö® Error in Group Refresh Rule: " + e);
}

return filtered;

import java.util.*;

try {
    // Log all incoming bindings
    log.error("=== DEBUG: Incoming Bindings ===");
    if (bindingVariables != null) {
        for (Object key : bindingVariables.keySet()) {
            log.error("Variable: " + key + " | Type: " + 
                (bindingVariables.get(key) == null ? "null" : bindingVariables.get(key).getClass().getName()));
        }
    } else {
        log.error("No binding variables found!");
    }

    if (groups == null) {
        log.error("‚ö†Ô∏è 'groups' is NULL or not being passed to this rule.");
    } else {
        log.error("‚úÖ Groups object received, Type: " + groups.getClass().getName());
    }
} catch (Exception e) {
    log.error("üö® Error during debug: " + e);
}
return null;


import java.util.*;
import java.util.stream.*;

List<Map<String, String>> results = new ArrayList<Map<String, String>>();

// Assuming 'response' is the JSON list returned from your connector call
// Replace this with your actual response fetching logic
for (Map<String, Object> record : response) {
    String permissions = (String) record.get("permissions");
    
    if (permissions != null) {
        String lowerPerm = permissions.toLowerCase();
        if (lowerPerm.contains("administration") ||
            lowerPerm.contains("approval queue") ||
            lowerPerm.contains("books closed") ||
            lowerPerm.contains("client onboarding") ||
            lowerPerm.contains("cusip inquiry") ||
            lowerPerm.contains("masterfile") ||
            lowerPerm.contains("pricing")) {
                
            results.add(record);
        }
    }
}

return results;


import java.util.*;
import sailpoint.object.Application;
import sailpoint.object.GroupDefinition;

List results = new ArrayList();

// Assuming the variable 'response' is a List of Map objects
if (response != null && response instanceof List) {
    for (int i = 0; i < response.size(); i++) {
        Object obj = response.get(i);
        if (obj instanceof Map) {
            Map record = (Map) obj;
            String permissions = (String) record.get("permissions");
            
            if (permissions != null) {
                String lowerPerm = permissions.toLowerCase();
                if (lowerPerm.contains("administration") ||
                    lowerPerm.contains("approval queue") ||
                    lowerPerm.contains("books closed") ||
                    lowerPerm.contains("client onboarding") ||
                    lowerPerm.contains("cusip inquiry") ||
                    lowerPerm.contains("masterfile") ||
                    lowerPerm.contains("pricing")) {
                    
                    // Add matching record to results
                    results.add(record);
                }
            }
        }
    }
}

return results;


import java.util.*;
import sailpoint.object.AccountGroup;

List results = new ArrayList();

if (response != null && response instanceof List) {
    for (int i = 0; i < response.size(); i++) {
        Object obj = response.get(i);
        if (obj instanceof Map) {
            Map record = (Map) obj;
            String permissions = (String) record.get("permissions");
            String mnemonic = (String) record.get("mnemonic");
            String userId = (String) record.get("userId");

            if (permissions != null) {
                String lowerPerm = permissions.toLowerCase();
                if (lowerPerm.contains("administration") ||
                    lowerPerm.contains("approval queue") ||
                    lowerPerm.contains("books closed") ||
                    lowerPerm.contains("client onboarding") ||
                    lowerPerm.contains("cusip inquiry") ||
                    lowerPerm.contains("masterfile") ||
                    lowerPerm.contains("pricing")) {

                    // Create AccountGroup object
                    AccountGroup group = new AccountGroup();
                    group.setNativeIdentity(mnemonic + "_" + permissions);
                    group.setDisplayName(permissions);
                    group.setAttribute("mnemonic", mnemonic);
                    group.setAttribute("userId", userId);
                    group.setAttribute("permissions", permissions);

                    results.add(group);
                }
            }
        }
    }
}

// Debug log
log.info("Filtered group results count: " + results.size());

return results;

