import sailpoint.object.Group;

public void refreshGroups(Iterator groups) {
    while (groups.hasNext()) {
        Group group = (Group) groups.next();

        String groupName = group.getName() != null ? group.getName() : "";

        // Filter out groups that do not match
        if (!(groupName.contains("Administration") ||
              groupName.contains("Approval Queue") ||
              groupName.contains("Books Closed") ||
              groupName.contains("Client Onboarding") ||
              groupName.contains("CUSIP Inquiry") ||
              groupName.contains("Masterfile") ||
              groupName.contains("Pricing"))) 
        {
            // Remove unwanted groups
            groups.remove();
        }
    }
}



import sailpoint.object.*;
import java.util.*;

// This rule filters groups during aggregation
// Only keeps entries where "permissions" contains specific keywords

List filtered = new ArrayList();

if (source != null) {
    Iterator it = source.iterator();

    while (it.hasNext()) {
        Map group = (Map) it.next();
        String perm = (group.get("permissions") != null) ? group.get("permissions").toString().toLowerCase() : "";

        // ‚úÖ Add your filtering keywords here
        if (perm.contains("administration") ||
            perm.contains("approval queue") ||
            perm.contains("client onboarding") ||
            perm.contains("books closed") ||
            perm.contains("cusip inquiry") ||
            perm.contains("masterfile") ||
            perm.contains("pricing")) {

            log.error("‚úÖ Keeping group: " + perm);
            filtered.add(group);
        } else {
            log.error("‚ùå Removing group: " + perm);
        }
    }
}

// Return filtered list
return filtered;



